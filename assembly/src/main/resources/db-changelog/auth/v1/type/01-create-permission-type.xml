<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="2025-02-15-create-gender-type" author="ulvusuleymanovv@gmail.com">
        <preConditions onFail="MARK_RAN">
            <sqlCheck expectedResult="f">
                select exists(select 1 from pg_catalog.pg_type where typname = 'permission');
            </sqlCheck>
        </preConditions>
        <sql>
            CREATE TYPE permission AS ENUM ('DELETE_POST', 'DELETE_ROOMMATE', 'DELETE_USER','BLOCK_USER');
        </sql>
        <rollback>
            <sql>DROP TYPE IF EXISTS permission;</sql>
        </rollback>
    </changeSet>

</databaseChangeLog>