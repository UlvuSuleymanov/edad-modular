<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="2025-02-14-create-gender-type" author="ulvu">
        <preConditions onFail="MARK_RAN">
            <sqlCheck expectedResult="f">
                select exists(select 1 from pg_catalog.pg_type where typname = 'gender');
            </sqlCheck>
        </preConditions>
        <sql>
            CREATE TYPE edadi.gender AS ENUM ('MALE', 'FEMALE', 'OTHER');
        </sql>
        <rollback>
            <sql>DROP TYPE IF EXISTS gender;</sql>
        </rollback>
    </changeSet>

</databaseChangeLog>